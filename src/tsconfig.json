{
	"compilerOptions": {
		"target": "ES5",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"declaration": false,
		"experimentalDecorators": true,
        "jsx": "react",
        "outDir": "./dist/",
		"noImplicitAny": true,
		"strictNullChecks": true,
		"lib": [
			"es2015", "dom"
		]
	},
	"exclude": [
        "dist",
		"node_modules"
	]
}
